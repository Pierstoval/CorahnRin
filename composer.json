        "easycorp/easyadmin-bundle": "^2.3",
        "orbitale/array-fixture": "^1.2",
        "pierstoval/character-manager": "^2.1",
        "symfony/asset": "^5.4@rc",
        "symfony/cache": "^5.4@rc",
        "symfony/config": "^5.4@rc",
        "symfony/console": "^5.4@rc",
        "symfony/dependency-injection": "^5.4@rc",
        "symfony/doctrine-bridge": "^5.4@rc",
        "symfony/dotenv": "^5.4@rc",
        "symfony/event-dispatcher": "^5.4@rc",
        "symfony/expression-language": "^5.4@rc",
        "symfony/filesystem": "^5.4@rc",
        "symfony/finder": "^5.4@rc",
