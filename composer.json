    "description": "Corahn-Rin project",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "rc",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "antishov/doctrine-extensions-bundle": "^1.4.3",
        "doctrine/common": "^3.1.0",
